            app:layout_constraintBottom_toTopOf="@+id/textView2"
            app:layout_constraintEnd_toEndOf="parent" />

        <TextView
            android:id="@+id/textView2"
            android:layout_width="730dp"
            android:layout_height="69dp"
            android:text="2 AÃ±os de Experiencia"
